{
  "name": "rugalib/ruga-log",
  "description": "Provides logging functionality for the ruga framework",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4",
    "psr/log": "^1.1",
    "laminas/laminas-config": "^3.7",
    "rugalib/ruga-std": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "repositories": [
    {"type": "vcs", "url": "https://github.com/rugalib/ruga-std.git"}
  ],
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruga\\Log\\Test\\": "test/src"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Log"
    }
  }
}
